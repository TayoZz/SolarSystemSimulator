    background-color: #343844; /* Slightly lighter on hover */
    border-color: #3D4250;
}

QPushButton:pressed {
    background-color: #1E2028; /* Darker pressed effect */
    border-color: #FFFFFF;
}
@keyframes glowPulse {
    0% { box-shadow: 0px 0px 5px rgba(122, 129, 255, 0.4); }
    50% { box-shadow: 0px 0px 15px rgba(122, 129, 255, 0.7); }
    100% { box-shadow: 0px 0px 5px rgba(122, 129, 255, 0.4); }
}

QPushButton:focus {
    outline: none;
    border: 2px solid #FFFFFF;
    animation: glowPulse 1.5s infinite alternate;
}</string>
