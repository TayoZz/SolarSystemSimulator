    border: 2px solid #3D4250; /* Subtle border */
    border-radius: 10px;
    padding: 10px 18px;
    transition: all 0.2s ease-in-out;
    box-shadow: 0px 4px 8px rgba(0, 0, 0, 0.3); /* Subtle depth effect */
}

